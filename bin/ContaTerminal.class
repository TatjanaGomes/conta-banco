import java.util.Scanner;public class ContaTerminal {   public static void main(String[] args) {      Scanner sc = new Scanner(System.in);      System.out.println("------------CONTA BANCÁRIA------------");      System.out.println();      System.out.println("Por favor, digite o número da Conta: (1234)");      int numero = sc.nextInt();      System.out.println("Agora digite o número da Agência: (123-8)");      String agencia = sc.next();      System.out.println("Me informe seu nome: ");      String nomeCliente = sc.next();      sc.nextLine();      System.out.println("Agora me informa seu saldo atual: ");      double saldo = sc.nextDouble();      System.out.println();      System.out.printf("Olá %s, obrigado por criar uma conta em nosso banco, sua agência é %s, conta %d e seu saldo %.2f já está disponível para saque.", nomeCliente, agencia, numero, saldo);      sc.close();   }}